<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/9.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.FrontController.UtenteBaseController">
   <children>
      <BorderPane prefHeight="250.0" prefWidth="400.0">
         <center>
            <Pane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <Label fx:id="txtruoloua" layoutX="322.0" layoutY="14.0" text="Utente Base" />
